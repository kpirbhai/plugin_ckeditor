{
    "name": "qcubed/plugin/ckeditor",
    "description": "QCubed plugin for CKEditor.",
    "type": "library",
    "keywords": ["qcubed", "ckeditor"],
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
      "psr-4": {
        "QCubed\\Plugin\\": "includes/",
        "QCubed\\Plugin\\QCKEditor": "composer/"
      }
    },
    "extra": {
        "examples": ["ckeditor.php"]
    },
    "scripts": {
       "post-package-install": [
         "QCubed\\Plugin\\QCKEditor\\Install::postPackageInstall"
       ],
      "post-package-update": [
        "QCubed\\Plugin\\QCKEditor\\Install::postPackageUpdate"
      ],
      "post-package-uninstall": [
        "QCubed\\Plugin\\QCKEditor\\Install::postPackageUninstall"
      ]
    }
}
